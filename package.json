{
  "name": "sails-code-navigator",
  "displayName": "Sails Code Navigator",
  "description": "The extension fixes code navigation issues caused by Sails globals",
  "author": {
    "name": "Illia Simenko",
    "email": "illia.simenko@redvike.com"
  },
  "publisher": "simenko-redvike",
  "repository": {
    "type": "git",
    "url": "https://github.com/simenko-redvike/sails-code-navigator"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "sails",
    "go to definition"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Sails Code Navigator Configuration",
      "properties": {
        "sailsCodeNavigator.definitionPaths": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "./api/services",
            "./api/models"
          ],
          "description": "An array of paths where the extension will search for files with definitions."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.92.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@babel/parser": "^7.25.6",
    "recast": "^0.23.9"
  },
  "license": "MIT"
}
